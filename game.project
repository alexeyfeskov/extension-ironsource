dependencies#5 = https://github.com/alexeyfeskov/extension-ironsource/archive/adapter-unityads.zip
dependencies#6 = https://github.com/alexeyfeskov/extension-ironsource/archive/adapter-vungle.zip

[library]
include_dirs = extension-ironsource

[android]
package = com.defold.IronSourceExample
minimum_sdk_version = 19

[render]
clear_color_red = 0.1
clear_color_green = 0.1
